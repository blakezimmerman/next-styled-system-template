{
  "name": "next-app",
  "version": "0.0.1",
  "scripts": {
    "dev": "next",
    "build": "next build",
    "start": "next start",
    "type-check": "tsc",
    "storybook": "start-storybook -s public -p 6006",
    "jest": "NODE_ENV=test jest --runInBand --watch --config jest.config.json",
    "jest-coverage": "NODE_ENV=test jest --runInBand --config jest.config.json",
    "postjest-coverage": "istanbul check-coverage --statements 0 --branches 0 --functions 0 --lines 0 ./coverage/coverage*.json",
    "view-coverage": "open ./coverage/index.html",
    "lint": "eslint 'src/**/*.{js,jsx,ts,tsx,mdx}'",
    "prettier:check": "prettier --check 'src/**/*.{js,jsx,ts,tsx,mdx}'",
    "prettier:fix": "prettier --write 'src/**/*.{js,jsx,ts,tsx,mdx}'",
    "is-valid": "CI=true run-p lint prettier:check jest-coverage build"
  },
  "dependencies": {
    "next": "9.5.3",
    "next-images": "^1.6.0",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-is": "^16.13.1",
    "styled-components": "^5.2.0",
    "styled-system": "^5.1.5"
  },
  "devDependencies": {
    "@storybook/addon-actions": "^6.0.25",
    "@storybook/addon-docs": "^6.0.25",
    "@storybook/addon-links": "^6.0.25",
    "@storybook/addons": "^6.0.25",
    "@storybook/react": "^6.0.25",
    "@testing-library/jest-dom": "^5.11.4",
    "@testing-library/react": "^11.0.4",
    "@types/node": "^12.12.21",
    "@types/react": "^16.9.50",
    "@types/react-dom": "^16.9.8",
    "@types/styled-components": "^5.1.3",
    "@types/styled-system": "^5.1.10",
    "@typescript-eslint/eslint-plugin": "^4.3.0",
    "@typescript-eslint/parser": "^4.3.0",
    "babel-plugin-styled-components": "^1.11.1",
    "eslint": "^7.10.0",
    "eslint-config-prettier": "^6.12.0",
    "eslint-plugin-jest-dom": "^3.2.3",
    "eslint-plugin-mdx": "^1.8.2",
    "eslint-plugin-react": "^7.21.3",
    "eslint-plugin-react-hooks": "^4.1.2",
    "eslint-plugin-testing-library": "^3.9.0",
    "istanbul": "^0.4.5",
    "jest": "^26.4.2",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.1.2",
    "ts-jest": "^26.4.1",
    "typescript": "4.0.3"
  },
  "license": "MIT"
}
